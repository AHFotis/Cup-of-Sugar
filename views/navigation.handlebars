<main class="pSection container photomargin col-md-9">
    <div class="row"></div>
    <div class="col-md-12"></div>
    <div class="col-md-12">
        {{#if name}}
        <h2 class="textcolor" style="margin: 20px">Welcome {{name}}!</h2>
        {{else}}
        <h2 class="textcolor">Cup Of Sugar</h2>
        {{/if}}
    </div>


    <div class="portfolio">
        <div class="row">

            <div class="col-sm-6">
                <picture class=" tooltip-wrap photomargin photo col-md-6">
                    <a class="btn" href="/navigation/asks">
                        <p class="tooltip-content">Community Forum</p>

                        <img src="/images/shutterstock_1160913544.jpg" class="rounded margin" width="150" height="150"
                            alt="Image community forum" />
                    </a>

                    <div>
                    </div>
                </picture>
            </div>

            <div class="col-sm-6">
                <picture class=" tooltip-wrap photomargin photo col-md-6">
                    <a class="btn" href="/navigation/resources">
                        <img src="/images/shutterstock_288532283.jpg" class="rounded margin" width="150" height="150"
                            alt="Resources" />
                        <p class="tooltip-content">Resources</p>
                    </a>

                </picture>
            </div>
        </div>

        {{#if loggedIn}}
        <picture class=" tooltip-wrap photomargin photo col-md-6">
            <a class="btn" href="/dashboard">
                <img src="/images/shutterstock_661961716.jpg" class="rounded margin" width="150" height="150"
                    alt="Image of wordweb dashboard" />
                <p class="tooltip-content">Dashboard</p>
            </a>

        </picture>
        {{/if}}
    </div>

</main>